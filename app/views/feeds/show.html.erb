<% content_for :head do %>
  <link rel="alternate" type="application/rss+xml" title="<%= @feed.title %>" href="<%= feed_url(@feed, format: :rss) %>" />
  <link rel="alternate" type="application/feed+json" title="<%= @feed.title %>" href="<%= feed_url(@feed, format: :json) %>" />
<% end %>

<%= render partial: 'shared/feed', locals: { feed: @feed, entry_limit: 100, display_mode: :extended } %>
